                                        <label
                                            class="pull-right content-clock">1 hour 27 min</label>
                                        <span class="boxicon mdi mdi-clock pull-right"></span>

                                        <span class="boxicon mdi mdi-wallet pull-left"></span>
                                                                                            <label
                                                class="pull-left">AED <%= cor.price %></label>
                                    </div>
                                </a>
                            </div>
                            <% inds = ind + 1 %>
                            <% if inds % 3 == 0 %>
                                <div class="h-10"></div>
                            <% end %>
                            <% end %>
                            
                        </div>
                        <div class="h-10"></div>
                        <div class="pagi text-center center-block col-xs-12"></div>
                        <div class="row pagi-s"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>