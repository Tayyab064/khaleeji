    <div class="h-30"></div>
    <div class="container-fluid">
        <div class="row">
            <div class="container">
                
                <div class="col-md-12 col-xs-12">
                    <div class="newest-container newest-container-s">
                    <div class="h-10"></div>
                        <div class="row">
                            <% @course.each_with_index do |cor, ind| %>
                            <div class="col-md-4 col-sm-6 col-xs-12 <%= ind==0 ? '' : 'tab-con' %>">
                                <a href="<%= specific_course_path(cor.id) %>" title="<%= cor.title %>"
                                   class="content-box pagi-content-box">

                                    <div class="img-container">
                                        <img src="<%= cor.image %>"" style="width: 100%; height: 230px;" />
                                                                                    </div>
                                    <h3><%= cor.title %></h3>
                                    <div class="footer">
                                        <span class="avatar" title="Lindsey Thaxton"
                                              onclick="window.location.href = '/profile/111'"><img
